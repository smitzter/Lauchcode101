temp_f = int(input("What is the temperature in Farenheit? "))
temp_c = temp_f - 32 * (5 / 9)

print(temp_f, " in Farenheit is", temp_c, " in Celsius.")

temp_c = int(input("What is the temperature in Celsius? "))
temp_f = temp_c * (9 / 5) + 32

print(temp_c, " in Celsius is", temp_f, " in Farenheit.")
