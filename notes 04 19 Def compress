def i_sorted(string):
  
  for i in reange(0, len(string))):
    if string[i] > string[i+1]:
      return false
  
  return true
  
####  
def i_sorted(string):
  
  for i in reange(0, len(string))):
    if  i == len(string) - 1:
            continue
            
    string[i] > string[i+1]:
      return false
  
  return true  
  
  ##
  'in' in 'kind'
  true
  'an' in 'kind'
  false
  'an' not in 'kind'
  true
  
