s = aabcccccaaa

def compress(s)
  count=0
  
  for(i) in range(0, len(s)):
  if s[i] == s[i-1]
  count += 1
  c = s.count(s[i])
  return str(s[i]) + str(c)
  
  
##Define Compress

s = 'aabcccccaaa'

def compress(string):

    cmp_str = ""
    count = 1
    
    #Add in first character
    cmp_str += string[0]

    #Iterate through loop, skipping last one
    for i in range(len(string)-1):
        if(string[i] == string[i+1]):
            count+=1
        else:
            if(count > 1):
                #Ignore if no repeats
                cmp_str += str(count)
            cmp_str += string[i+1]
            count = 1
    #print last one
    if(count > 1):
        cmp_str += str(count)
    return cmp_str
print(compress(s))
    ###
    
def compress(string):
  cmp_str = ""
  last_char = -1
  count = 0
    
  for char in string:   
    if last_char == -1:
      last_char == char:
      count += 1
    else:
      comp_str += last_char + str(count)
      last_char = char
      count = 1
      
  comp_str += last_char + str(count)
  if len(comp_str) . len(string):
    return string
      
  return cmp_str
  
